#!/bin/sh

set -e

case "$1" in
    purge)
        rm -f /var/lib/systemd/catalog/database
        rmdir --ignore-fail-on-non-empty /var/lib/systemd/catalog 2> /dev/null || true

        if getent group systemd-journal >/dev/null; then
            delgroup systemd-journal || true
        fi
    ;;
esac

